    }, {
      "componentAttributes" : {
        "richTextValue" : "<div style=\"text-align: center; padding: 40px; background-color: #ffffff; font-family: Arial, sans-serif;\">\n    <h2 style=\"color: #005c75; font-size: 32px; font-weight: bold; margin-bottom: 20px;\">\n        Educational Services\n    </h2>\n    <div style=\"display: flex; justify-content: flex-start; align-items: center; gap: 40px; flex-wrap: wrap;\">\n        <!-- Text Section (Left) -->\n        <div style=\"max-width: 500px; text-align: left; margin-left: 10%;\">\n            <p style=\"color: #6c757d; font-size: 20px; line-height: 1.8; margin-top:40px; margin-bottom: 20px;\">\n                Increase productivity and efficiency through online training courses designed to give you a deeper \n                understanding of your Quest product. Earn certifications and become your organization’s Quest expert.\n            </p>\n            <a href=\"/educational-service\" \n               style=\"display: inline-block; padding: 14px 30px; background-color: #ff5722; color: #ffffff; \n                      text-decoration: none; font-weight: bold; \n                      transition: background-color 0.3s ease; font-size: 16px;\">\n                Learn More\n            </a>\n        </div>\n        \n        <div style=\"flex: 1; max-width: 600px; text-align: center; margin-left: 0;\">\n    <img src=\"https://www.quest.com/images/patterns/ZigZag/6-column/educational-services.svg\" \n         alt=\"Educational Services Illustration\" \n         style=\"width: 100%; height: auto; max-height: 350px;\" />\n</div>\n\n    </div>\n</div>\n\n\n<div style=\"text-align: center; padding: 40px; background-color: #f9f9f9; font-family: Arial, sans-serif;\">\n    <h2 style=\"color: #005c75; font-size: 32px; font-weight: bold; margin-bottom: 40px;\">\n        Support Services\n    </h2>\n    <div style=\"display: flex; justify-content: center; align-items: center; gap: 40px; flex-wrap: wrap;\">\n        <div style=\"flex: 1; min-width: 300px; max-width: 600px; text-align: center;\">\n            <img src=\"https://www.quest.com/images/patterns/ZigZag/6-column/support-services.svg\" \n                 alt=\"Support Services Illustration\" \n                 style=\"max-width: 100%; height: auto;\" />\n        </div>\n        <div style=\"flex: 1; min-width: 300px; max-width: 500px; text-align: left;\">\n            <p style=\"color: #6c757d; font-size: 18px; line-height: 1.8; margin-bottom: 20px;\">\n                Our award-winning support team provides deep product knowledge as well as extensive domain expertise. \n                Their expert technical assistance saves you time so you can focus on what’s important to your business.\n            </p>\n            <a href=\"/CustomerServicePortalVersion2/s/support-service\" \n               style=\"display: inline-block; padding: 15px 30px; background-color: #ff5722; color: #ffffff; \n                      text-decoration: none; font-weight: bold; border-radius: 6px; \n                      transition: background-color 0.3s ease; font-size: 16px;\">\n                Learn More\n            </a>\n        </div>\n    </div>\n</div>"
